<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ns:Envelope xmlns:ns="http://www.w3.org/2003/05/soap-envelope"
             xmlns:ns0="http://schemas.xmlsoap.org/ws/2002/07/secext">
    <ns:Header>
        <ns0:UsernameToken>
            <ns0:Username>Alice</ns0:Username>
        </ns0:UsernameToken>
    </ns:Header>
    <!-- pre body -->
    <ns:Body>
        <ns:ComplexInheritance xmlns:ns="http://k5.eu/tr/model/inheritance"
                               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <!-- before type -->
            <ns:type ns:baseAttribute="baseAttrValue" ns:subAttribute="subAttrValue" xsi:type="ns:SubType">
                <!-- before base value -->
                <ns:baseElement>baseValue</ns:baseElement>
                <!-- before subvalue -->
                <ns:subElement>subValue</ns:subElement>
                <!-- after subvalue -->
            </ns:type>
        </ns:ComplexInheritance>
    </ns:Body>
    <!-- post body -->
</ns:Envelope>
